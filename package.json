{
  "name": "cdgplayer",
  "version": "0.1.6",
  "description": "A browser based HTML5/JS karaoke 'Player', that takes a zip file containing an mp3 and cdg file, allowing the user to render and play directly in the browser.",
  "main": "dist/cdgplayer.js",
  "module": "dist/cdgplayer.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "start": "node ./server.js"
  },
  "keywords": [
    "cdg",
    "cd+g",
    "karaoke"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cutterbl/CDGPlayer.git"
  },
  "author": {
    "name": "Stephen G Blades, Jr (AKA 'Cutter')",
    "url": "http://cutterscrossing.com"
  },
  "license": "GPL-3.0-or-later",
  "files": [
    "dist/**/*",
    "index.html",
    "index.js",
    "server.js",
    "CDGPlayer.svg"
  ],
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "@babel/core": "7.10.5",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@fortawesome/fontawesome-free": "5.14.0",
    "@fortawesome/fontawesome-svg-core": "1.2.30",
    "@fortawesome/free-regular-svg-icons": "5.14.0",
    "@rollup/plugin-commonjs": "14.0.0",
    "@rollup/plugin-node-resolve": "8.4.0",
    "acorn": "^7.1.1",
    "autoprefixer": "9.8.5",
    "babel-eslint": "10.1.0",
    "connect": "3.7.0",
    "eslint": "7.5.0",
    "node-sass": "4.14.1",
    "open": "7.1.0",
    "postcss-assets": "^5.0.0",
    "prettier": "2.0.5",
    "regenerator-runtime": "0.13.5",
    "rollup": "2.22.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-cleanup": "3.1.1",
    "rollup-plugin-clear": "2.0.7",
    "rollup-plugin-embed-css": "1.0.18",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-html": "0.2.1",
    "rollup-plugin-postcss": "3.1.3",
    "rollup-plugin-terser": "6.1.0",
    "sass": "1.26.10",
    "serve-static": "1.14.1",
    "soundtouchjs": "0.1.21"
  },
  "dependencies": {
    "jsmediatags": "3.9.3",
    "jszip": "3.5.0",
    "jszip-utils": "0.1.0"
  },
  "resolutions": {
    "tar": ">=4.4.2"
  }
}
