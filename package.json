{
  "name": "cdgplayer",
  "version": "0.1.1",
  "description": "A browser based HTML5/JS karaoke 'Player', that takes a zip file containing an mp3 and cdg file, allowing the user to render and play directly in the browser.",
  "main": "dist/cdgplayer.js",
  "module": "dist/cdgplayer.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "start": "node ./server.js"
  },
  "keywords": [
    "cdg",
    "cd+g",
    "karaoke"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": {
    "name": "Stephen G Blades, Jr (AKA 'Cutter')",
    "url": "http://cutterscrossing.com"
  },
  "license": "GPL-3.0-or-later",
  "files": [
    "dist/**/*",
    "index.html",
    "index.js",
    "server.js",
    "CDGPlayer.svg"
  ],
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-regular-svg-icons": "^5.7.2",
    "autoprefixer": "^9.4.10",
    "babel-eslint": "^10.0.1",
    "connect": "^3.6.6",
    "eslint": "^5.15.1",
    "node-sass": "^4.11.0",
    "postcss-assets": "^5.0.0",
    "prettier": "^1.16.4",
    "regenerator-runtime": "^0.13.1",
    "rollup": "^1.4.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-embed-css": "^0.4.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "sass": "^1.17.2",
    "serve-static": "^1.13.2",
    "soundtouchjs": "^0.1.5"
  },
  "dependencies": {
    "jsmediatags": "^3.8.1",
    "jszip": "^3.2.0",
    "jszip-utils": "0.0.2"
  }
}
